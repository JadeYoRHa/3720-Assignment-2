{
    "manifest_version": 3,
    "name": "UltFrameData Modifier",
    "description": "Extension to modify frame data in accordance with the options used in game",
    "version": "1.0",
    "icons": {
        "16": "images/Logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://ultimateframedata.com/sf6/*"
            ],
            "css": [
                "styles.css"
            ],
            "js": [
                "scripts.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}